@model ProjectCalidadSoft.Models.DiagnosticoMedico

@{
    ViewData["Title"] = "Delete";
}

<h1>Delete</h1>

<h3>Estas segurdo de eliminar?</h3>
<div>
    <h4>DiagnosticoMedico</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Paciente)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Paciente.Id)
        </dd class>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Cie10)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Cie10.Codigo)
        </dd class>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Fecha)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Fecha)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="IdPaciente" />
        <input type="hidden" asp-for="IdCie10" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Regresar</a>
    </form>
</div>
